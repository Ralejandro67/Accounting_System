@model PupuseriaSalvadorena.Models.TipoTransacciones

<form id="TipoTForm" asp-action="Create" asp-controller="TipoTransacciones" method="post">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="form-group">
        <label asp-for="TipoTransac" class="control-label"></label>
        <input asp-for="TipoTransac" class="form-control" />
        <span asp-validation-for="TipoTransac" class="text-danger"></span>
    </div>
    <div class="form-group text-right">
        <button type="button" id="submitTipoTForm" class="btn btn-primary">Agregar</button>
    </div>
</form>
