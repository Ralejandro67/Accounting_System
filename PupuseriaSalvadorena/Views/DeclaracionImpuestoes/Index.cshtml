            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaInicio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaFinal)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MontoTotalIngresos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MontoTotalEgresos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MontoTotalImpuestos)
