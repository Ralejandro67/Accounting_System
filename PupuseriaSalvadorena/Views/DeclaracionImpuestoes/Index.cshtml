                @Html.DisplayFor(modelItem => item.CedulaJuridica)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaInicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaFinal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MontoTotalIngresos)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MontoTotalEgresos)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MontoTotalImpuestos)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Observaciones)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdDeclaracionImpuesto">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdDeclaracionImpuesto">Details</a> |
